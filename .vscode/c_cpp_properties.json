{
  "configurations": [
    {
      "name": "Win32",
      "includePath": [
        "${workspaceFolder}/**",
        "${workspaceFolder}/src/**",
        "${workspaceFolder}/node_modules/@types/**"
      ],
      "defines": ["_DEBUG", "UNICODE", "_UNICODE", "WIN32"],
      "windowsSdkVersion": "10.0.19041.0",
      "compilerPath": "cl.exe",
      "cStandard": "c23",
      "cppStandard": "c++26",
      "intelliSenseMode": "windows-msvc-x64",
      "browse": {
        "path": [
          "${workspaceFolder}",
          "${workspaceFolder}/src",
          "${workspaceFolder}/node_modules"
        ],
        "limitSymbolsToIncludedHeaders": true,
        "databaseFilename": ""
      }
    }
  ],
  "version": 4
}
